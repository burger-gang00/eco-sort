# EcoSort Frontend Environment Variables

# API Configuration
VITE_API_URL=http://localhost:5001/api

# App Configuration
VITE_APP_NAME=EcoSort
VITE_APP_DESCRIPTION="Smart Waste Management System"

# Environment
VITE_NODE_ENV=development

# App Version
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Ola Maps Configuration
VITE_OLA_MAPS_API_KEY=your_ola_maps_api_key_here
VITE_OLA_MAPS_PROJECT_ID=your_project_id_here
VITE_OLA_MAPS_CLIENT_ID=your_client_id_here
VITE_OLA_MAPS_CLIENT_SECRET=your_client_secret_here
