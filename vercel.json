{
  "version": 2,
  "name": "ecosort",
  "builds": [
    {
      "src": "frontend/ecosort/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "cd frontend/ecosort && npm run build",
  "outputDirectory": "frontend/ecosort/dist",
  "installCommand": "cd frontend/ecosort && npm install --legacy-peer-deps"
}
